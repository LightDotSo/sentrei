  "tasksRunnerOptions": {
    "default": {
      "options": {
        "accessToken": "NmMzOTQzOWUtNTM1MS00N2Y2LWIzMmMtNzRlZjZjOTFhMDQxfHJlYWQtd3JpdGU=",
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "canTrackAnalytics": true,
        "showUsageWarnings": true
      },
      "runner": "@nrwl/nx-cloud"
    }
  },
  "workspaceLayout": {
    "libsDir": "components"
  }
