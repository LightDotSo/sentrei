      "tags": ["scope:configuration"]
    },
    "husky": {
      "tags": ["scope:configuration"]
    },
    "jest": {
      "tags": ["scope:configuration"]
    },
    "lint-staged": {
      "tags": ["scope:configuration"]
    },
    "next": {
      "tags": ["scope:configuration"],
      "implicitDependencies": ["react"]
    },
    "npm-check-updates": {
      "tags": ["scope:configuration"]
    },
    "npm-package-json-lint": {
      "tags": ["scope:configuration"]
