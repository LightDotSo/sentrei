{
  "affected": {
    "defaultBase": "origin/main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.js": "*",
    "tsconfig.base.json": "*"
  },
  "npmScope": "sentrei",
  "projects": {
    "atoms": {
      "tags": ["scope:component"]
    },
    "molecules": {
      "tags": ["scope:component"],
      "implicitDependencies": ["atoms"]
    },
    "organisms": {
      "tags": ["scope:component"],
      "implicitDependencies": ["molecules"]
    },
    "roots": {
      "tags": ["scope:component"]
    },
    "screens": {
      "tags": ["scope:component"],
      "implicitDependencies": ["atoms", "molecules", "organisms"]
    },
    "app": {
      "tags": ["scope:app"],
      "implicitDependencies": [
        "atoms",
        "molecules",
        "organisms",
        "roots",
        "screens"
      ]
    },
    "app-e2e": {
      "tags": ["scope:e2e"],
      "implicitDependencies": ["app"]
    },
    "demo": {
      "tags": ["scope:app"],
      "implicitDependencies": [
        "atoms",
        "molecules",
        "organisms",
        "roots",
        "screens"
      ]
    },
    "demo-e2e": {
      "tags": ["scope:e2e"],
      "implicitDependencies": ["demo"]
    },
    "home": {
      "tags": ["scope:app"],
      "implicitDependencies": [
        "atoms",
        "molecules",
        "organisms",
        "roots",
        "screens"
      ]
    },
    "home-e2e": {
      "tags": ["scope:e2e"],
      "implicitDependencies": ["home"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "NzA4MGQzMzUtYTI5Ny00MGU4LWJkM2UtMjJiMjkwM2ZkMzQ1fHJlYWQtd3JpdGU=",
        "canTrackAnalytics": true,
        "showUsageWarnings": true
      }
    }
  }
}
