        "molecules",
        "organisms",
        "roots",
        "screens",
        "storybook",
        "typescript",
        "webpack"
      ]
    },
    "demo-video": {
      "tags": ["scope:video"],
      "implicitDependencies": ["remotion"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "canTrackAnalytics": true,
        "showUsageWarnings": true
      }
    }
  },
  "workspaceLayout": {
    "libsDir": "components"
  }
}
