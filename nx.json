{
  "affected": {
    "defaultBase": "origin/main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "nx.json": "*",
    "tsconfig.base.json": "*"
  },
  "npmScope": "sentrei",
  "projects": {
    "atoms": {
      "tags": ["scope:component"]
    },
    "molecules": {
      "tags": ["scope:component"],
      "implicitDependencies": ["atoms"]
    },
    "organisms": {
      "tags": ["scope:component"],
      "implicitDependencies": ["molecules"]
    },
    "roots": {
      "tags": ["scope:component"]
    },
    "screens": {
      "tags": ["scope:component"],
      "implicitDependencies": ["atoms", "molecules", "organisms"]
    },
    "babel": {
      "tags": ["scope:configuration"]
    },
    "commitlint": {
      "tags": ["scope:configuration"]
    },
    "cypress": {
      "tags": ["scope:configuration"]
    },
    "depcheck": {
      "tags": ["scope:configuration"]
    },
    "dev": {
      "tags": ["scope:configuration"]
    },
    "eslint": {
      "tags": ["scope:configuration"]
    },
    "husky": {
      "tags": ["scope:configuration"]
    },
    "jest": {
      "tags": ["scope:configuration"]
    },
    "lint-staged": {
      "tags": ["scope:configuration"]
    },
    "next": {
      "tags": ["scope:configuration"],
      "implicitDependencies": ["react"]
    },
    "npm-check-updates": {
      "tags": ["scope:configuration"]
    },
    "npm-package-json-lint": {
      "tags": ["scope:configuration"]
    },
    "nrwl": {
      "tags": ["scope:configuration"]
    },
    "postcss": {
      "tags": ["scope:configuration"]
    },
    "prettier": {
      "tags": ["scope:configuration"]
    },
    "renovate": {
      "tags": ["scope:configuration"]
    },
    "semantic-release": {
      "tags": ["scope:configuration"]
    },
    "storybook": {
      "tags": ["scope:configuration"]
    },
    "tailwindcss": {
      "tags": ["scope:configuration"]
    },
    "typescript": {
      "tags": ["scope:configuration"]
    },
    "vite": {
      "tags": ["scope:configuration"]
    },
    "webpack": {
      "tags": ["scope:configuration"]
    },
    "app": {
      "tags": ["scope:app"],
      "implicitDependencies": [
        "atoms",
        "molecules",
        "organisms",
        "roots",
        "screens",
        "next"
      ]
    },
    "app-e2e": {
      "tags": ["scope:e2e"],
      "implicitDependencies": ["app"]
    },
    "demo": {
      "tags": ["scope:app"],
      "implicitDependencies": [
        "atoms",
        "molecules",
        "organisms",
        "roots",
        "screens",
        "next"
      ]
    },
    "demo-e2e": {
      "tags": ["scope:e2e"],
      "implicitDependencies": ["demo"]
    },
    "home": {
      "tags": ["scope:app"],
      "implicitDependencies": [
        "atoms",
        "molecules",
        "organisms",
        "roots",
        "screens",
        "next"
      ]
    },
    "home-e2e": {
      "tags": ["scope:e2e"],
      "implicitDependencies": ["home"]
    },
    "design": {
      "tags": ["scope:app"],
      "implicitDependencies": ["storybook"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "tasksRunnerOptions": {
