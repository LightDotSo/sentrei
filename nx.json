{
  "affected": {
    "defaultBase": "origin/main"
  },
  "implicitDependencies": {
    "nx.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "yarn.lock": "*"
  },
  "npmScope": "sentrei",
  "projects": {
    "atoms": {
      "tags": ["scope:component"]
    },
    "layouts": {
      "tags": ["scope:component"]
    },
    "molecules": {
      "tags": ["scope:component"]
    },
    "organisms": {
      "tags": ["scope:component"]
    },
    "roots": {
      "tags": ["scope:component"]
    },
    "screens": {
      "tags": ["scope:component"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "projects": "dependencies",
        "target": "build"
      }
    ]
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "accessToken": "NmMzOTQzOWUtNTM1MS00N2Y2LWIzMmMtNzRlZjZjOTFhMDQxfHJlYWQtd3JpdGU=",
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "canTrackAnalytics": true,
        "showUsageWarnings": true
      },
      "runner": "@nrwl/nx-cloud"
    }
  },
  "workspaceLayout": {
    "libsDir": "components"
  }
}
