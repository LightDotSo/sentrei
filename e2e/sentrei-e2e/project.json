{
  "projectType": "application",
  "root": "e2e/sentrei-e2e",
  "sourceRoot": "e2e/sentrei-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "e2e/sentrei-e2e/cypress.json",
        "tsConfig": "e2e/sentrei-e2e/tsconfig.e2e.json",
        "devServerTarget": "sentrei:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "sentrei:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["e2e/sentrei-e2e/**/*.{js,ts}"]
      }
    }
  }
}
