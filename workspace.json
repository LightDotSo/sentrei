{
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "defaultProject": "home",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "atoms": "components/atoms",
    "molecules": "components/molecules",
    "organisms": "components/organisms",
    "roots": "components/roots",
    "screens": "components/screens",
    "next": "configurations/next",
    "storybook": "configurations/storybook",
    "design": ".storybook",
    "app": "apps/app",
    "demo": "apps/demo",
    "home": "apps/home",
    "app-e2e": "e2e/app-e2e",
    "demo-e2e": "e2e/demo-e2e",
    "home-e2e": "e2e/home-e2e"
  },
  "version": 2,
  "workspaceLayout": {
    "libsDir": "libs",
    "appsDir": "apps"
  }
}
