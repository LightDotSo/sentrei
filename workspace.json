{
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "defaultProject": "home",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "atoms": "components/atoms",
    "molecules": "components/molecules",
    "organisms": "components/organisms",
    "roots": "components/roots",
    "screens": "components/screens",
    "babel": "configurations/babel",
    "commitlint": "configurations/commitlint",
    "cypress": "configurations/cypress",
    "dev": "configurations/dev",
    "eslint": "configurations/eslint",
    "husky": "configurations/husky",
    "jest": "configurations/jest",
    "lint-staged": "configurations/lint-staged",
    "next": "configurations/next",
    "npm-check-updates": "configurations/npm-check-updates",
    "npm-package-json-lint": "configurations/npm-package-json-lint",
    "nrwl": "configurations/nrwl",
    "postcss": "configurations/postcss",
    "prettier": "configurations/prettier",
    "react": "configurations/react",
    "remotion": "configurations/remotion",
    "renovate": "configurations/renovate",
    "semantic-release": "configurations/semantic-release",
    "sentry": "configurations/sentry",
    "storybook": "configurations/storybook",
    "stylelint": "configurations/stylelint",
    "tailwindcss": "configurations/tailwindcss",
    "typescript": "configurations/typescript",
    "vite": "configurations/vite",
    "webpack": "configurations/webpack",
    "design": ".storybook",
    "app-sentrei-com": "apps/app-sentrei-com",
