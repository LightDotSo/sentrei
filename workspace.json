    "demo": "apps/demo",
    "docs": "apps/docs",
    "home": "apps/home",
    "og-sentrei-com": "apps/og-sentrei-com",
    "www-mosh-lol": "apps/www-mosh-lol",
    "app-sentrei-com-e2e": "e2e/app-sentrei-com-e2e",
