{
  "projectType": "library",
  "root": "components/roots",
  "sourceRoot": "components/roots/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["components/roots/**/*.ts"]
      }
    },
    "test": {
